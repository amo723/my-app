{
  "expo": {
    "name": "MADIBA",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.madibacorp.madiba"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.madibacorp.madiba",
      "permissions": ["INTERNET"],
      "debuggable": true,
      "versionCode": 1,
      "icon": "./assets/madiba-icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/madiba-icon.png", 
        "backgroundColor": "#FFFFFF"  
      }
    },
    "adaptiveIcon": {
      "foregroundImage": "./assets/madiba-icon.png",
      "backgroundColor": "#ffffff"
    },
    "plugins": ["expo-secure-store"],
    "network": {
      "allowExternalNetworking": true
    },
    "extra": {
      "enableDebugging": true,
      "eas": {
        "projectId": "defac88a-49ac-4dd4-8e6c-c4c40507fca2"
      }
    },
    "owner": "amo237"
  }
}
