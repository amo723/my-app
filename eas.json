{
  "cli": {
    "version": ">= 10.2.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development"
      },
      "image": "ghcr.io/expo/expo-docker:latest"  // Spécifie l'image Docker à utiliser pour les builds
    },
    "preview": {
      "distribution": "internal",
      "image": "ghcr.io/expo/expo-docker:latest"  // Spécifie l'image Docker à utiliser pour les builds
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "env": {
        "NODE_ENV": "production"
      },
      "image": "ghcr.io/expo/expo-docker:latest"  // Spécifie l'image Docker à utiliser pour les builds
    }
  },
  "submit": {
    "production": {}
  }
}
